{
  "name": "reactbot0",
  "version": "0.0.0",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "backend": "nodemon ./bin/www",
    "frontend": "npm run start --prefix react-client0",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix react-client0 && npm run build --prefix react-client0"
  },
  "dependencies": {
    "actions-on-google": "^2.13.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dialogflow": "^1.2.0",
    "dialogflow-fulfillment": "^0.6.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.10.3",
    "morgan": "~1.9.1",
    "structjson": "^1.0.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
